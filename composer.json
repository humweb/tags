{
    "name": "humweb/tags",
    "description": "Add tags and taggable behaviour to your Laravel app",
    "keywords": [
        "spatie",
        "laravel-tags"
    ],
    "homepage": "https://github.com/humweb/tags",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        },
        {
            "name": "Ryun Shofner",
            "email": "ryun@humweboldtweb.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.12",
        "spatie/laravel-package-tools": "^1.4"
    },
    "require-dev": {
        "orchestra/testbench": "^6.13",
        "phpunit/phpunit": "^9.5.2"
    },
    "autoload": {
        "psr-4": {
            "Humweb\\Tags\\": "src"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Humweb\\Tags\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Humweb\\Tags\\TagsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
